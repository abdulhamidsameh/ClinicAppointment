@model DoctorViewModel

<div class="form-group mb-5">
    <label asp-for="FullName" class="form-label"></label>
    <input readonly asp-for="FullName" class="form-control" />
</div>
<div class="form-group mb-5">
    <label asp-for="Gender" class="form-label"></label>
    <input readonly asp-for="Gender" class="form-control" />
</div>
<div class="form-group mb-5">
    <label asp-for="Clinic.Name" class="form-label">Clinic Name</label>
    <input readonly asp-for="Clinic.Name" class="form-control" />
</div>
<div hidden class="form-group">

    <label class="col-form-label" asp-for="ClinicId"></label>
    <input asp-for="ClinicId" class="form-control" readonly />
</div>